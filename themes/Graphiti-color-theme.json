{
	"tokenColors": [
		{
			"settings": {
				"foreground": "#F8F8F2",
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#F8F8F2",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"entity.name.filename"
			],
			"settings": {
				"foreground": "#78dce8",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"support.constant"
			],
			"settings": {
				"foreground": "#78dce8"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"meta.object-literal",
				"support.type.property-name",
				"support.variable",
				"support.other.variable",
				"entity.other",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#ff6188",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage",
				"entity.other.attribute-name.id"
			],
			"settings": {
				"foreground": "#ffd866"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage.modifier",
				"storage.type"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ffd866"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"support.function",
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#a9dc76"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#aaaaaa"
			}
		},


		{
			"name": "diff.header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#655c2d"
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#ff6188"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#a9dc76"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#ffd866"
			}
		}
	],
	"colors": {
		"editor.background": "#252525",
